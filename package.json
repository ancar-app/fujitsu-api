{
  "name": "@ancar-app/fujitsu-api",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/ancar-app/fujitsu-api.git"
  },
  "main": "dist/index.js",
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "api:build": "aspida",
    "lint": "eslint src/*",
    "lint:fix": "eslint src/* --fix",
    "build": "aspida --build && tsc --build tsconfig.json"
  },
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "peerDependencies": {
    "aspida": "^1.3.0"
  },
  "devDependencies": {
    "@types/eslint": "^7.2.6",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/prettier": "^2.1.6",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "aspida": "^1.3.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-aspida": "^0.0.2",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "git add"
    ]
  }
}
